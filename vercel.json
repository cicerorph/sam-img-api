{
  "version": 2,
  "builds": [
    {
      "src": "api/image.js",
      "use": "@vercel/node"
    }
  ],
  "build": {
    "env": {
      "NODE_VERSION": "20"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/api/image.js"
    },
    {
      "src": "/api",
      "dest": "/api/image.js"
    },
    {
      "src": "/api/perfil",
      "dest": "/api/image.js"
    },
    {
      "src": "/api/rank",
      "dest": "/api/image.js"
    },
    {
      "src": "/api/atm",
      "dest": "/api/image.js"
    }
  ]
}